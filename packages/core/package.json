{
  "name": "@sarathcani999/mugglekit",
  "version": "2.0.0",
  "description": "Core utilities and components for the Muggle Kit monorepo",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc -p tsconfig.build.json --watch",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "publish-dry": "npm publish --dry-run"
  },
  "devDependencies": {
    "@repo/typescript-config": "*",
    "typescript": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sarathcani999/muggle-kit.git",
    "directory": "packages/core"
  },
  "author": "Sarath C Ani mailsforsarath98@gmail.com",
  "license": "MIT",
  "keywords": [
    "typescript",
    "core",
    "utilities",
    "mugglekit"
  ],
  "publishConfig": {
    "access": "public"
  }
}
